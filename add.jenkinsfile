pipeline{
	agent any
	parameters{
		string(name: 'a', defaultValue: '0', description:"Enter value >=1")
		string(name: 'b', defaultValue: '0', description:"Enter value >=1")
	}
	stages{
		stage('addition'){
			steps{
				sh """docker-compose -f test/docker-compose.yml run --rm add_two_num /opt/add/ addition.py --a=${params.a} \
							--b=${params.b}"""
			}
		}
	}
}
